{
  "functions": {
    "app/auth/signin/page.tsx": {
      "maxDuration": 1
    },
    "app/auth/signup/page.tsx": {
      "maxDuration": 1
    }
  },
  "headers": [
    {
      "source": "/auth/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "no-cache, no-store, must-revalidate"
        },
        {
          "key": "Pragma",
          "value": "no-cache"
        },
        {
          "key": "Expires",
          "value": "0"
        }
      ]
    }
  ],
}
